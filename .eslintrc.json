{
    "env": {
        "browser": true,
        "es6": true,
        "es2017": true,
        "es2020": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "script"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": "warn"
    },
    "globals": {
        "window": "readonly",
        "document": "readonly",
        "console": "readonly",
        "setTimeout": "readonly",
        "clearInterval": "readonly",
        "setInterval": "readonly",
        "navigator": "readonly"
    }
}